{
	"languageserver": {
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
			"rootPatterns": [".ccls", ".ccls-root", "compile_commands.json", ".git/", ".hg/"],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				}
			}
		},
		"rust": {
			"command": "rust-analyzer",
			"filetypes": ["rust"],
			"rootPatterns": ["Cargo.toml"]
		},
		"lua": {
			"command": "lua-lsp",
			"filetypes": ["lua"]
		}
	},

//	"rust-analyzer.updates.prompt": false,
//	"rust-analyzer.server.path": "~/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/bin/rust-analyzer",

	// coc-rls for RUST

"rust-client.enable": true,
"rust-client.logToFile": false,
"rust-client.setLibPath": true,
"rust-client.rustupPath": "rustup",
"rust-client.rlsPath": null,
"rust-client.revealOutputChannelOn": "never", //Valid options: ["info","warn","error","never"]
"rust-client.updateOnStartup": false,
"rust-client.disableRustup": false,
"rust-client.channel": null, //Valid options: ["stable","beta","nightly"]
"rust-client.trace.server": "off", //Valid options: ["off","messages","verbose"]
"rust.sysroot": null,
"rust.target": null,
"rust.rustflags": null,
"rust.clear_env_rust_log": true,
"rust.build_lib": null,
"rust.build_bin": null,
"rust.cfg_test": false,
"rust.unstable_features": false,
"rust.wait_to_build": 1500,
"rust.show_warnings": true,
"rust.crate_blacklist": ["cocoa","gleam","glium","idna","libc","openssl","rustc_serialize","serde","serde_json","typenum","unicode_normalization","unicode_segmentation","winapi"]
"rust.build_on_save": false,
"rust.features": [],
"rust.all_features": false,
"rust.no_default_features": false,
"rust.racer_completion": true,
"rust.clippy_preference": "opt-in", //Valid options: ["on","opt-in","off"]
"rust.jobs": null,
"rust.all_targets": true,
"rust.target_dir": null,
"rust.rustfmt_path": null,
"rust.build_command": null,
"rust.full_docs": null,
"rust.show_hover_context": true,

// coc-rust-analyzer for RUST
"rust-analyzer.enable": true,
"rust-analyzer.server.path": "''",
"rust-analyzer.updates.prompt": true,
"rust-analyzer.updates.channel": "nigntly",
"rust-analyzer.updates.checkOnStartup": true,
"rust-analyzer.diagnostics.enable": true,
"rust-analyzer.diagnostics.enableExperimental": true,
"rust-analyzer.diagnostics.disabled": [],
"rust-analyzer.diagnostics.warningsAsInfo": [],
"rust-analyzer.diagnostics.warningsAsHint": [],
"rust-analyzer.experimental.procAttrMacros": false,
"rust-analyzer.lruCapacity": null,
"rust-analyzer.inlayHints.enable": true,
"rust-analyzer.inlayHints.typeHints": true,
"rust-analyzer.inlayHints.typeHintsSeparator": "‚Ä£",
"rust-analyzer.inlayHints.chainingHints": true,
"rust-analyzer.inlayHints.chainingHintsSeparator": "‚Ä£",
"rust-analyzer.inlayHints.refreshOnInsertMode": false,
"rust-analyzer.files.watcher": "client",
"rust-analyzer.notifications.cargoTomlNotFound": true,
"rust-analyzer.cargo.autoreload": true,
"rust-analyzer.cargo.allFeatures": false,
"rust-analyzer.cargo.features": [],
"rust-analyzer.cargo.noDefaultFeatures": false,
"rust-analyzer.cargo.runBuildScripts": false,
"rust-analyzer.procMacro.enable": false,
"rust-analyzer.rustfmt.extraArgs": [],
"rust-analyzer.rustfmt.overrideCommand": null,
"rust-analyzer.rustfmt.enableRangeFormatting": false,
"rust-analyzer.checkOnSave.enable": true,
"rust-analyzer.checkOnSave.target": null,
"rust-analyzer.checkOnSave.extraArgs": [],
"rust-analyzer.checkOnSave.command": "check",
"rust-analyzer.checkOnSave.overrideCommand": null,
"rust-analyzer.checkOnSave.allTargets": true,
"rust-analyzer.checkOnSave.noDefaultFeatures": null,
"rust-analyzer.checkOnSave.allFeatures": null,
"rust-analyzer.checkOnSave.features": null,
"rust-analyzer.completion.addCallParenthesis": true,
"rust-analyzer.completion.addCallArgumentSnippets": true,
"rust-analyzer.completion.postfix.enable": true,
"rust-analyzer.completion.autoimport.enable": true,
"rust-analyzer.lens.enable": true,
"rust-analyzer.lens.run": true,
"rust-analyzer.lens.implementations": true,
"rust-analyzer.lens.methodReferences": false,
"rust-analyzer.hoverActions.linksInHover": false,
"rust-analyzer.assist.importGranularity": "crate",
"rust-analyzer.assist.importEnforceGranularity": false,
"rust-analyzer.assist.allowMergingIntoGlobImports": true,
"rust-analyzer.assist.importPrefix": "plain",
"rust-analyzer.assist.importGroup": true,
"rust-analyzer.callInfo.full": true,
"rust-analyzer.trace.server": "off",
"rust-analyzer.debug.runtime": "termdebug",
"rust-analyzer.debug.vimspector.configuration.name": "launch",

	"coc.preferences.extensionUpdateCheck": "daily",
